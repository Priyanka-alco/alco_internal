<p id="notice"><%= notice %></p>

<h1>Paytm Details</h1>

<table>
  <thead>
    <tr>
      <th>Phone number</th>
      <th>Cust</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @paytm_details.each do |paytm_detail| %>
      <tr>
        <td><%= paytm_detail.phone_number %></td>
        <td><%= paytm_detail.cust_id %></td>
        <td><%= paytm_detail.status %></td>
        <td><%= link_to 'Show', paytm_detail %></td>
        <td><%= link_to 'Edit', edit_paytm_detail_path(paytm_detail) %></td>
        <td><%= link_to 'Destroy', paytm_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Paytm Detail', new_paytm_detail_path %>
